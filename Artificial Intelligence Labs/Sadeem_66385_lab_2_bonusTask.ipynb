        "#Bonus Task\n",
        "fruit_list=[\"apple\",\"orange\",\"mango\",\"banana\",\"apple\",\"banana\"]\n",
        "seen=set()\n",
        "repeats=set()\n",
        "for fruit in fruit_list:\n",
        "    if fruit in seen:\n",
        "        repeats.add(fruit)\n",
        "    else:\n",
        "        seen.add(fruit)\n",
        "print(repeats)"
